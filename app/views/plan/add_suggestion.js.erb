$('.add-suggestion[data-accreditation-id="<%= @accreditation.id %>"]').addClass('disabled').attr('disabled',true);
$('.accreditations-list').append('<%= j render("accreditation_detail", accreditation: @accreditation) %>');
$('.subscription-btn-container').html('<%= j render("subscription_btn", accreditations_ids: @accreditations_ids, role: @role) %>');

$('.add-suggestion').each(function(i,btn) {
  var $btn = $(btn);
  var id = $btn.data('accreditation-id')
  var urlTemplate = '<%= add_suggestion_plan_path("XXX",role: @role,accreditations: @accreditations_ids) %>'
  $(btn).attr('href',urlTemplate.replace('XXX',id))
});
